.jn-timeline {
  width: 70%;
  min-width: 700px;
  margin: 30px auto;
  position: relative;
}

.jn-timeline-item {
  position: relative;
  padding-bottom: 30px;
  min-height: 70px;
}
.jn-timeline-item:before {
  content: "";
  position: absolute;
  top: 0px;
  left: 50%;
  width: 3px;
  height: 100%;
  background: #919191;
}

.jn-timeline-item-name {
  position: absolute;
  left: 12px;
  width: 44%;
  font-size: 20px;
  color: #222222;
  top: 10px;
  text-align: right;
}

.jn-timeline-item-icon {
  position: absolute;
  top: 0;
  left: 50%;
  height: 85px;
  margin-left: -35px;
  font-size: 70px;
  color: #919191;
  background: #fff;
  cursor: pointer;
}

.jn-timeline-item-content {
  position: relative;
  top: 15px;
  left: 55%;
  width: 44%;
  background: #eceaea;
  padding: 25px 25px 10px 25px;
  -webkit-box-shadow: 0 3px 0 rgba(0, 0, 0, 0.1);
  -moz-box-shadow: 0 3px 0 rgba(0, 0, 0, 0.1);
  -ms-box-shadow: 0 3px 0 rgba(0, 0, 0, 0.1);
  -o-box-shadow: 0 3px 0 rgba(0, 0, 0, 0.1);
  box-shadow: 0 3px 0 rgba(0, 0, 0, 0.1);
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -ms-border-radius: 5px;
  -o-border-radius: 5px;
  border-radius: 5px;
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -ms-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  -o-box-sizing: border-box;
  box-sizing: border-box;
}
.jn-timeline-item-content > h1 {
  padding: 8.33333px 25px;
  background: #919191;
  color: white;
  margin-top: -25px;
  margin-bottom: 0;
  margin-left: -25px;
  margin-right: -25px;
  font-size: 20px;
  font-weight: 100;
  -webkit-border-radius: 3px 3px 0 0;
  -moz-border-radius: 3px 3px 0 0;
  -ms-border-radius: 3px 3px 0 0;
  -o-border-radius: 3px 3px 0 0;
  border-radius: 3px 3px 0 0;
}
.jn-timeline-item-content > button {
  padding: 5px 15px;
  margin-top: 5px;
  text-decoration: none;
  background: transparent;
  border: 2px solid #004b97;
  color: #004b97;
  display: inline-block;
  position: relative;
  font-size: 12px;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -ms-border-radius: 5px;
  -o-border-radius: 5px;
  border-radius: 5px;
  cursor: pointer;
}
.jn-timeline-item-content > button:hover {
  border: 2px solid #ee4d4d;
  color: #ee4d4d;
}
.jn-timeline-item-content > .jn-timeline-item-content-panel {
  margin-top: 5px;
  font-size: 13px;
  line-height: 20px;
}

.jn-arrow-left {
  position: absolute;
  right: 100%;
  top: 15px;
  border-top: 7px solid transparent;
  border-bottom: 7px solid transparent;
  border-right: 7px solid #919191;
}

.jn-arrow-right {
  position: absolute;
  left: 100%;
  top: 15px;
  border-top: 7px solid transparent;
  border-bottom: 7px solid transparent;
  border-left: 7px solid #919191;
}

.jn-timeline-content-left {
  left: 0px;
}

.jn-timeline-item-name-left {
  left: 56%;
  font-size: 23.33333px;
  top: 25px;
  text-align: left;
}

.jn-timeline-item-small {
  padding-bottom: 10px;
}
.jn-timeline-item-small > .jn-timeline-item-content {
  top: 10px;
}
.jn-timeline-item-small > .jn-timeline-item-icon {
  margin-left: -21px;
  font-size: 42px;
  height: 57px;
}

.jn-timelime-item-line-hide:before {
  display: none;
}

.jn-timeline-no-content {
  width: 100%;
  min-width: 100px;
}
.jn-timeline-no-content .jn-timeline-item:before {
  left: 55px;
}
.jn-timeline-no-content .jn-timeline-item > .jn-timeline-item-icon {
  left: 55px;
}
.jn-timeline-no-content .jn-timeline-item > .jn-timeline-item-name {
  text-align: left;
  left: 100px;
  width: 60%;
  top: 25px;
}
.jn-timeline-no-content .jn-timeline-item-small > .jn-timeline-item-icon {
  right: 35px;
}
.jn-timeline-no-content .jn-timeline-item-small > .jn-timeline-item-name {
  top: 15px;
}

.jn-timeline-all-right {
  min-width: 190px;
}
.jn-timeline-all-right .jn-timeline-item:before, .jn-timeline-all-right .jn-timeline-item > .jn-timeline-item-icon {
  left: 0;
}
.jn-timeline-all-right .jn-timeline-item > .jn-timeline-item-content {
  width: 90%;
  left: 50px !important;
}
.jn-timeline-all-right .jn-timeline-item > .jn-timeline-item-name {
  left: -250px;
  width: 200px;
  top: 25px;
}
.jn-timeline-all-right .jn-timeline-item-small > .jn-timeline-item-content {
  top: 10px;
}
.jn-timeline-all-right .jn-timeline-item-small > .jn-timeline-item-name {
  top: 15px;
}

.jn-timeline-item-color-red > .jn-timeline-item-content > h1 {
  background: #ee4d4d;
}
.jn-timeline-item-color-red > .jn-timeline-item-icon {
  color: #ee4d4d;
}
.jn-timeline-item-color-red .jn-arrow-left {
  border-right: 7px solid #ee4d4d;
}
.jn-timeline-item-color-red .jn-arrow-right {
  border-left: 7px solid #ee4d4d;
}

.jn-timeline-item-color-blue:before, .jn-timeline-item-color-blue > .jn-timeline-item-content > h1 {
  background: #004b97;
}
.jn-timeline-item-color-blue > .jn-timeline-item-icon {
  color: #004b97;
}
.jn-timeline-item-color-blue .jn-arrow-left {
  border-right: 7px solid #004b97;
}
.jn-timeline-item-color-blue .jn-arrow-right {
  border-left: 7px solid #004b97;
}

.jn-timeline-item-color-yellow > .jn-timeline-item-content > h1 {
  background: #ff540d;
}
.jn-timeline-item-color-yellow > .jn-timeline-item-icon {
  color: #ff540d;
}
.jn-timeline-item-color-yellow .jn-arrow-left {
  border-right: 7px solid #ff540d;
}
.jn-timeline-item-color-yellow .jn-arrow-right {
  border-left: 7px solid #ff540d;
}


